<div class="container">  <div class="row">    <div class="col-xs-12 col-sm-4 col-sm-offset-4">      <div class="sign-in-wrapper">        <div class="sign-in-header text-center">          <h2>Log in</h2> <hr>        </div>        <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>          <div class="form-inputs">            <%= f.input :email, required: false, autofocus: true %>            <%= f.input :password, required: false %>            <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>          </div>          <div class="form-actions">            <%= f.button :submit, "Log in" %> <hr>            <div class="form-actions-links">              <%= link_to 'Forgot your password ?', new_password_path(resource_name) %>              | <%= link_to '  Sign up  ', new_user_registration_path %>            </div>          </div>        <% end %>        <%= render "devise/shared/links" %>    </div>  </div></div>