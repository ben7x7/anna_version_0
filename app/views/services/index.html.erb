<div class="container-index-service">  <div class="row">    <div class="text-center">      <h3>Easy booking form inline coming soon here !</h3>    </div>    <% @services.each do |service| %>    <div class="col-xs-12 col-sm-6 center">      <div class="wrapper-card-service">        <div class="card-service">          <div class="card-service-activity">            <h3> <%= service.activity %> </h3>          </div>          <div class="card-service-picto">            <%= image_tag "#{service.activity.downcase}.png" %>          </div>          <div class="card-service-link">            <%= link_to '', service_path(service), class: "card-service-link" %>          </div>        </div>      </div>    </div>    <% end %>  </div>  <div class="bottom-links">    <%= link_to 'Home', root_path %>  </div></div>